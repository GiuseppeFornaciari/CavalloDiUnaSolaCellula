<form name="FD4" method="post" action="">

    @for(int i = 0; i < 36; i++){
        @Html.TextBox("G" + i.ToString(), i, new { style = "width:40px;margin:1px" });
    }

</form>
<script type="text/javascript" language="javascript">
//<![CDATA[
    self.setInterval('numerillo_D4()', 1);
    function numerillo_D4() {
        td4 = 'G' + Math.floor(Math.random() * 36);
        document.FD4.elements[td4].value = Math.floor(Math.random() * 1000);
    }
//]]>
</script>
